#include <stdio.h>
int main() {
    int a[10],i,n,max,min;
    printf("Enter n value ");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    max=a[0];
    min=a[0];
    for(i=1;i<n;i++){
        
        if(a[i]>max){
            max=a[i];
        }
        
        if(a[i]<min){
            min=a[i];
        }
    }
    printf("Max element is : %d\n",max);
    printf("Min element is : %d\n",min);
   
   return 0;
}
